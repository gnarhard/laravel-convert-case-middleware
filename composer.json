{
    "name": "gnarhard/laravel-convert-case-middleware",
    "version": "1.2.1",
    "description": "Convert request and response keys to and from camel and snake case.",
    "type": "library",
    "require": {
        "php": ">=8.0",
        "laravel/framework": "^12.0",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.5",
        "pestphp/pest": "^4.3"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Tom Lerendu",
            "email": "tom.lerendu@gmail.com"
        },
        {
            "name": "Grayson Erhard",
            "email": "develop@graysonerhard.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "TomLerendu\\LaravelConvertCaseMiddleware\\": "src/",
            "TomLerendu\\LaravelConvertCaseMiddleware\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
